    "claude",
    "claude-cli",
    "gemini",
    "gemini-cli",
    "zai",
    "z.ai",
    "ai",
    "limit-checker",
    "api-monitoring",
    "usage-monitoring",
    "openai-alternative",
    "anthropic",
    "google-ai",
    "cli-tool",
    "typescript"
