  "author": "Shaharia Lab <hello@shaharia.com> (https://github.com/shaharia-lab)",
  "license": "MIT",
  "homepage": "https://github.com/shaharia-lab/ai-limit-checker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaharia-lab/ai-limit-checker.git"
  },
  "bugs": {
    "url": "https://github.com/shaharia-lab/ai-limit-checker/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "node-pty": "^1.1.0",
    "playwright": "^1.49.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
